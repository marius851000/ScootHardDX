Class SHDX_BlueVase : SHDX_Prop
{
	Default
	{
		//$NotAngled
		//$Title "Blue Vase"
		//$Sprite VASEA0
		//$Category "Obstacles"
		
		Radius 10;
		Height 26;
		Health 5;
		
		+SHOOTABLE
		+NOBLOOD
	}
	
	States
	{
		Spawn:
			VASE A -1;
			Stop;
			
		Death:
			VASE A 0
			{
				A_NoBlocking();
				A_StartSound("misc/tcanbreak", CHAN_BODY);
				A_SpawnMetalJunkFx();
			}
			VASE A 5;
			Stop;
	}
}

